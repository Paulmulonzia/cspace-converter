<div class="row">

  <%= render partial: 'shared/sidebar' %>

  <div class="col-md-10">

    <%= render partial: 'shared/title', :locals => { :title => 'Object' } %>

    <div class="row">
      <div class="col-md-12">
        <h2><%= object_label(@object) %></h2>
      </div>
    </div>

    <div class="row">
      <div class="col-md-12">
        <% @object.procedure_objects.each do |procedure| %>
          <p><%= procedure.type %> <%= link_to procedure.identifier, procedure_path(procedure) %></p>
        <% end %>
      </div>
    </div>

    <div class="row">
      <div class="col-md-12">
        <p>Return to <%= link_to 'Objects', objects_path %></p>
      </div>
    </div>

  </div>

</div>