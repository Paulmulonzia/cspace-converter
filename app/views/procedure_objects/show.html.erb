<div class="row">

  <%= render partial: 'shared/sidebar' %>

  <div class="col-md-10">

    <%= render partial: 'shared/title', :locals => { :title => @object.identifier } %>

    <div class="row">
      <div class="col-md-12">
        <p>Parent: <%= object_label(@object.data_object) %> <%= link_to "View", object_path(@object.data_object) %></p>
      </div>
    </div>

    <div class="row">
      <div class="col-md-12">
        <pre class="xml"><%= @object.content %></pre>
      </div>
    </div>

    <div class="row">
      <div class="col-md-12">
        <p>Return to <%= link_to 'Procedures', procedures_path %></p>
      </div>
    </div>

  </div>

</div>